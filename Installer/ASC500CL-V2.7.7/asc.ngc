            <Widget Type="Xedit"                                   X="0" Y="3" Address="0x0053" Unit="%" Prefix="X" Min="0" Max="100" Persist="1"/>
          </Widget>
        </Widget>
      </Tab>
      
      <Tab  Caption="Counter" Margin="10" >
        <Widget Type="Label" Caption="Exposure Time"           X="0" Y="0" Alignment="Left" />
        <Widget Type="Xedit"                                   X="1" Y="0" Unit="s" Prefix="u" Address="0x310" Index="0" Min="0" Max="163837.0" Scale="0.4" Persist="1" />
      </Tab>
      
      <Tab Caption="AFM"  Margin="10">
        <Widget Type="Label" Caption="AFM AC Input Range"      X="0" Y="0"  Alignment="Left" />
        <Widget Type="Combo"                                   X="1" Y="0"  Address="0x0350" Index="0" Choices="5V:20V" Persist="1" />
      </Tab>
      
    </Widget>
  </Dialog>
  
  
  <Dialog Caption="Transfer Functions ..." Modal="0" Popup="Menu:Settings" >
    <Macro Type="Repeater" Count="6" >
