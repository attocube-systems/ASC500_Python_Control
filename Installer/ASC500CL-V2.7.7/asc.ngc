<Panel Name="Main Control" Icon="scanner.xpm"  >
  
  <Widget Type="Constant" Address="0x0257" Index="3"  DefVal="24" />   <!-- Selects Frequency DAC as Destination for spectroscopy 4, used by calibration -->
  <Widget Type="Constant" Address="0x3B"   Index="10" DefVal="7"  />   <!-- Channel 10 is in fix use by line view  -->
  <Widget Type="Constant" Address="0x32"   Index="10" DefVal="400"/>   <!-- Channel 10 Data Rate 1kHz  -->
  <Widget Type="Constant" Address="0x39"   Index="10" DefVal="1"  />   <!-- Channel 10 is locked and not available for dynamic channel management -->
  <Widget Type="Constant" Address="0x3B"   Index="12" DefVal="8"  />   <!-- Channel 12 is in fix use by FFT  -->
  <Widget Type="Constant" Address="0x32"   Index="12" DefVal="10" />   <!-- Channel 12 Data Rate 40kHz -->
  <Widget Type="Constant" Address="0x39"   Index="12" DefVal="1"  />   <!-- Channel 12 is locked and not available for dynamic channel management -->
  
  <!-- Safe state on shutdown -->
  <Dialog Caption="Shutdown Devices"                Modal="0" Popup="Close" >
    <Widget Type="Button" Caption="Shutdown Outputs"      X="1" Y="0" Address="0x141" Toggle="0" Data="0" />
    <Widget Type="Label"  Caption="" Size="30"            X="0" Y="0" />
    <Widget Type="Label"  Caption="" Size="30"            X="2" Y="0" />
    <Widget Type="Box" Style="Flat"                       X="0" Y="1" Width="3"  Spacing="10" >
      <Widget Type="Label" Caption="Output Status:"          X="0" Y="0" Align="VCenter" />
      <Widget Type="Led"   Caption=""                        X="1" Y="0" Address="0x140" PopupCond="!=0" />
    </Widget>
  </Dialog>

  <Dialog Caption="Experiment Preferences ..." Modal="0" Popup="Menu:Settings" >
    <Widget Type="Tabs" >
      
      <Tab Caption="Feedback Clocks" Margin="10"  >
        <Widget Type="Box" Style="Flat" Align="VCenter">
          <Widget Type="Label" Caption="Z Feedback Ts"              X="0" Y="0" Alignment="Left" Condition="==(ZFeedback,1)"/>
          <Widget Type="Combo"                                      X="1" Y="0" Address="0x7F" Index="0" Offset="3" Choices="20 탎:40 탎:80 탎:160 탎:320 탎:640 탎:1.28ms:2.56ms"   Persist="1" Condition="==(ZFeedback,1)" />
          <Widget Type="Label" Caption="Amplitude Feedback Ts"      X="0" Y="1" Alignment="Left" Condition="==(AFM,1)" />
          <Widget Type="Combo"                                      X="1" Y="1" Address="0xFE" Index="0" Offset="3" Choices="20 탎:40 탎:80 탎:160 탎:320 탎:640 탎:1.28ms:2.56ms"   Persist="1" Condition="==(AFM,1)" />
          <Widget Type="Label" Caption="Frequency Feedback Ts"      X="0" Y="2" Alignment="Left" Condition="==(AFM,1)" />
          <Widget Type="Combo"                                      X="1" Y="2" Address="0xFF" Index="0" Offset="3" Choices="20 탎:40 탎:80 탎:160 탎:320 탎:640 탎:1.28ms:2.56ms"   Persist="1" Condition="==(AFM,1)" />
          <Widget Type="Label" Caption="Gen. 1 Feedback Ts"         X="0" Y="3" Alignment="Left" Condition="==(AFM,1)" />
          <Widget Type="Xedit"                                      X="1" Y="3" Address="0x1C5" Index="0" Unit="s" Prefix="u"    Min="20.0"   Max="163837.0" Scale="0.4" Persist="1" Condition="==(AFM,1)" />
          <Widget Type="Label" Caption="Gen. 2 Feedback Ts"         X="0" Y="4" Alignment="Left" Condition="==(AFM,1)" />
          <Widget Type="Xedit"                                      X="1" Y="4" Address="0x1C5" Index="1" Unit="s" Prefix="u"    Min="20.0"   Max="163837.0" Scale="0.4" Persist="1" Condition="==(AFM,1)" />
        </Widget>
      </Tab>

      <Tab Caption="Scanner" Margin="10" >
        <Widget Type="Box" Style="Flat"                        X="0" Y="0" Align="VCenter" Condition="!(band(ScanRun,6))" HideOnCond="0">
          <Widget Type="Box"    Caption="Closed Loop Sensor"     X="0" Y="0"  Condition="==(ClLoop,1)" >
            <Widget Type="Check"  Caption="Invert X"               X="0" Y="0" Address="0x2F9" Index="0" Persist="1" />
            <Widget Type="Check"  Caption="Invert Y"               X="0" Y="1" Address="0x2FA" Index="0" Persist="1" />
            <Widget Type="Check"  Caption="Use LVDS"               X="0" Y="2" Address="0x0B6" Index="0" Persist="1" />
            <Widget Type="Label" Caption="Closed Loop Sensitivity"    X="0" Y="3" Alignment="Left" Condition="==(ClLoop,1)" />
            <Widget Type="Xedit"                                      X="1" Y="3" Width="2" Address="0x30E" Index="0" Unit="" Prefix=" " Scale="1" FixPrefix="On"   Min="0"   Max="100"  Persist="1" Condition="==(ClLoop,1)" />
          </Widget>
          <Widget Type="Box"    Caption="Scanner Acceleration"   X="0" Y="1" >
            <Widget Type="Label" Alignment="Left"                  X="0" Y="0" Caption="Acceleration (0 means &infin;)"/>
            <Widget Type="Xedit"                                   X="0" Y="1" Address="0x0052" Unit="m/s/s" Prefix="u" Min="0" Max="2000000000" Persist="1"/>
