          <Widget Type="Check" Caption="Enable"            X="1" Y="0" Address="0x40" Index="$Counter$" Persist="1"/>
          <Widget Type="Label" Caption="Bias Comp."        X="0" Y="1" Alignment="Left"/>
          <Widget Type="Xedit" Caption=""                  X="1" Y="1" Unit="V" Prefix="m" Address="0x41" Index="$Counter$" Scale="3.2767" Min="-10000" Max="10000" Persist="1" />
