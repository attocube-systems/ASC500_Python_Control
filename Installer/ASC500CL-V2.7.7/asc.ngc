      <Tab Name="Scan"  >
        <Widget Type="Box" Style="Flat" Align="Top Left">
          <Widget Type="Box" Caption="Manual Positioning"          X="0" Y="0" >
            <Widget Type="Box" Style="Flat"                          X="0" Y="0" Width="3" >
              <Widget Type="Label"    Caption="Stepwidth"              X="0" Y="0" Alignment="Left" />
              <Widget Type="Xedit"    Caption=""                       X="1" Y="0"  Unit="m" Prefix="n" Address="0x1067" Index="0"  Scale="100."  Persist="1" Width="2" />
            </Widget>
            <Widget Type="Button" Icon="arrow_up.xpm"                X="1" Y="1"            Address="0x1068" Index="0" Accel="CTRL+Up"    />
            <Widget Type="Button" Icon="arrow_down.xpm"              X="1" Y="3"            Address="0x1069" Index="0" Accel="CTRL+Down"  />
            <Widget Type="Button" Icon="arrow_left.xpm"              X="0" Y="2"            Address="0x106A" Index="0" Accel="CTRL+Left"  />
            <Widget Type="Button" Icon="arrow_right.xpm"             X="2" Y="2"            Address="0x106B" Index="0" Accel="CTRL+Right" />
          </Widget>
          <Widget Type="Box" Caption="Absolute Positioning"          X="0" Y="1" >
            <Widget Type="Label"  Caption="X"                          X="0" Y="0" Alignment="Left" />
            <Widget Type="Label"  Caption="Y"                          X="0" Y="1" Alignment="Left" />
            <Widget Type="Xedit"                                       X="1" Y="0" Width="3" Unit="m" Prefix="n" Address="0x0027" Index="0"  Scale="100." />
            <Widget Type="Xedit"                                       X="1" Y="1" Width="3" Unit="m" Prefix="n" Address="0x0028" Index="0"  Scale="100." />
            <Widget Type="Button" Caption="Move"                       X="0" Y="2" Width="4" Align="HCenter"     Address="0x003A" Index="0" />
          </Widget>

          <Widget Type="Box"    Style="Box" Caption="Closed Loop"  X="1" Y="0" Condition="==(ClLoop,1)" >
            <Widget Type="Check"  Caption="Use Sensor Position"      X="0" Y="0" Width="2" Address="0x1FE" Index="0" Align="VCenter" Persist="1" Condition="!(OutActive)" HideOnCond="0" />
