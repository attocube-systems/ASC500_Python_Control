            <Widget Type="Led"    Caption=""                         X="1" Y="1" Address="0x2D5" DefaultColor="Red" Red="1" Grey="0" Align="VCenter Left" Tip="Feedback Saturation" />
            <Widget Type="Button" Caption="Reset"                    X="3" Y="0" Address="0x2F6" Toggle="0" Tip="Reset position" />
            <Widget Type="Button" Caption="Restore"                  X="3" Y="1" Address="0x2F8" Toggle="0"/>
            <Widget Type="Box" Style="Flat"                          X="0" Y="2" Width="4" Align="Bottom">
              <Widget Type="Label"  Caption="Sensor Position X"        X="0" Y="0" Align="Left"/>
              <Widget Type="Label"  Caption="Sensor Position Y"        X="0" Y="1" Align="Left"/>
              <Widget Type="Label"  Caption=""        		       X="1" Y="0" Align="Right" Address="0x2ED" Unit=" 탆" Scale="100000" Display="Float" Accuracy="3"/>
              <Widget Type="Label"  Caption=""                         X="1" Y="1" Align="Right" Address="0x2EE" Unit=" 탆" Scale="100000" Display="Float" Accuracy="3"/>
            </Widget>
          </Widget>
        </Widget>
      </Tab>
      
      
