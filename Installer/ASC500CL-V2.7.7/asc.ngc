            <Widget Type="Combo"   Caption=""              X="1" Y="2" Address="0x1044" Index="$Counter$" Choices="$Units$" Persist="1"/>
          </Widget>
        </Widget>
        
        <Widget Type="Box"   Caption="Internal Transfer Function"  X="0" Y="1"  Style="Box"   >  
          <Widget Type="Box"                               X="2" Y="0"  Style="Flat"   >  
            <Widget Type="Label" Caption="Factor"          X="0" Y="0" Alignment="Left"/>
            <Widget Type="Xedit" Caption=""                X="1" Y="0" Unit="" Prefix="X"  Address="0x1040" Index="$Counter$" Scale="1000000." Persist="1" />
            <Widget Type="Label" Caption="Offset"          X="0" Y="1" Alignment="Left"/>
