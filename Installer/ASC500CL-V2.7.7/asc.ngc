          <Widget Type="Combo" Caption=""                  X="1" Y="2" Address="0x42" Index="$Counter$" Choices="x1:x4:x16:x64" Persist="1"/>
        </Widget>
      </Widget>
    </Macro>
    <!-- Fuer die zwei Extra-ADCs gibt es keine Bias Comp -->
    <Macro Type="Repeater" Count="2" >
      <Widget Type="Box"   Caption="ADC$+(Counter,7)$"    X="$+(Counter,2)$" Y="1"  Style="Box" Condition="EightAdc" Align="Top" >
